version: "3.9"

services:
  db:
    image: mysql:8.0
    container_name: catrescue-db
    restart: unless-stopped
    environment:
      MYSQL_ROOT_PASSWORD: "123456"
      MYSQL_DATABASE: "cat_rescue"
